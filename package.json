{
  "name": "seng-513-back-end",
  "description": "SENG 513 Final Project",
  "version": "0.0.1",
  "private": true,
  "main": "app.js",
  "engines": {
    "node": "12.x.x",
    "npm": "6.x.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint:fix"
    ]
  },
  "scripts": {
    "start": "node ./app.js",
    "watch": "nodemon --exec ts-node src/app.js",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "build": "rm -rf build && tsc && node ./generateAssets.js"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-async-handler": "1.1.4",
    "firebase-admin": "8.9.2",
    "http-errors": "1.7.3",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "morgan": "1.9.1",
    "shelljs": "0.8.3",
    "uuid": "3.4.0",
    "validator": "12.2.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.6",
    "@types/express": "4.17.2",
    "@types/http-errors": "1.6.3",
    "@types/lodash": "4.14.149",
    "@types/morgan": "1.7.37",
    "@types/validator": "12.0.1",
    "@typescript-eslint/eslint-plugin": "2.19.2",
    "@typescript-eslint/parser": "2.19.2",
    "eslint": "6.8.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.2",
    "lint-staged": "10.0.7",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "ts-node": "8.6.2",
    "typescript": "3.7.5"
  }
}
